<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <!-- Define a vehicle type for our simulation -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" minGap="2.5" maxSpeed="30" color="1,0,0"/>
    
    <!-- Define a simple route -->
    <route id="route0" edges="e1 e2"/>
    
    <!-- Add some vehicles -->
    <vehicle id="veh0" type="car" route="route0" depart="0" />
    <vehicle id="veh1" type="car" route="route0" depart="5" />
    <vehicle id="veh2" type="car" route="route0" depart="10" />
    <vehicle id="veh3" type="car" route="route0" depart="15" />
</routes>